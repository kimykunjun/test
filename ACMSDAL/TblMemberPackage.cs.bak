             //  " Where F.strPackageCode ='Unlinked' and A.strMemberShipID =@strMembershipID ";

                if (dt2.Rows.Count > 0)
                {
                    cmdtext = " Select A.*, D.strEmployeeName, B.strDescription, B.nMaxSession, C.nCategoryID, C.strDescription as CategoryDescription, B.strFreePkgCode " +
