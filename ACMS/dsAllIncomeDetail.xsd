		,sum(mAmount) As TotalAmount,	
		cast(sum(mAmount)/(SELECT dTaxRate + 1.00 FROM tblTax WHERE nTaxID = CASE WHEN dtDate &lt; '1-jul-2007' THEN 2 ELSE 1 END) as numeric(8,2))  As mNettAmount
		
		from 
		DWV_AllIncome_Detail
		
		Where strBranchCode In 
		(Select strBranchCode from tblEmployeeBranchRights Where nEmployeeID = @empID) and nReportGroupID = @nSalesCategoryID 
		and month(dtDate) = @month and year(dtDate)= @year and day(dtdate) &lt;= @day
